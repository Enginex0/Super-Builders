+#ifdef CONFIG_KSU_SUSFS_UNICODE_FILTER
+bool susfs_check_unicode_bypass(const char __user *filename) {
+	char buf[256];
+	int i, len;
+	unsigned char c;
+	uid_t uid = current_uid().val;
+
+	if (uid == 0 || uid == 1000)
+		return false;
+
+	len = strncpy_from_user(buf, filename, sizeof(buf));
+	if (len <= 0 || len >= sizeof(buf))
+		return false;
+
+	for (i = 0; i < len; i++) {
+		c = (unsigned char)buf[i];
+		if (c < 0x80)
+			continue;
+		if (i + 2 < len) {
+			unsigned char c1 = (unsigned char)buf[i+1];
+			unsigned char c2 = (unsigned char)buf[i+2];
+			/* RTL/LTR override (U+202E, U+202D) */
+			if (c == 0xE2 && c1 == 0x80 && (c2 == 0xAE || c2 == 0xAD)) {
+				SUSFS_LOGI("unicode bypass blocked (RTL/LTR override) uid=%u\n", uid);
+				return true;
+			}
+			/* zero-width space (U+200B), ZWNJ (U+200C), ZWJ (U+200D) */
+			if (c == 0xE2 && c1 == 0x80 && (c2 >= 0x8B && c2 <= 0x8D)) {
+				SUSFS_LOGI("unicode bypass blocked (zero-width) uid=%u\n", uid);
+				return true;
+			}
+			/* BOM (U+FEFF) */
+			if (c == 0xEF && c1 == 0xBB && c2 == 0xBF) {
+				SUSFS_LOGI("unicode bypass blocked (BOM) uid=%u\n", uid);
+				return true;
+			}
+		}
+		/* Cyrillic ranges (U+0400-U+04FF  0xD0-0xD1 lead byte) */
+		if (c == 0xD0 || c == 0xD1) {
+			SUSFS_LOGI("unicode bypass blocked (Cyrillic) uid=%u\n", uid);
+			return true;
+		}
+		/* Diacritical marks (U+0300-U+036F  0xCC-0xCD lead byte) */
+		if (c == 0xCC || c == 0xCD) {
+			SUSFS_LOGI("unicode bypass blocked (diacritical) uid=%u\n", uid);
+			return true;
+		}
+	}
+	return false;
+}
+#endif
