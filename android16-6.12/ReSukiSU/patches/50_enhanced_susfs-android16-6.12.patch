+/* hidden ino hashtable */
+struct susfs_hidden_ino_entry {
+	unsigned long ino;
+	struct super_block *sb;
+	struct hlist_node node;
+	struct rcu_head rcu;
+};
+
+static DEFINE_HASHTABLE(HIDDEN_INO_HLIST, 10);
+
+bool susfs_is_hidden_ino(struct super_block *sb, unsigned long ino) {
+	struct susfs_hidden_ino_entry *entry;
+
+	rcu_read_lock();
+	hash_for_each_possible_rcu(HIDDEN_INO_HLIST, entry, node, ino) {
+		if (entry->ino == ino && entry->sb == sb) {
+			rcu_read_unlock();
+			return true;
+		}
+	}
+	rcu_read_unlock();
+	return false;
+}
+EXPORT_SYMBOL(susfs_is_hidden_ino);
+
+/* hidden name hashtable */
+struct susfs_hidden_name_entry {
+	char name[SUSFS_MAX_LEN_PATHNAME];
+	int namlen;
+	uid_t owner_uid;
+	struct hlist_node node;
+	struct rcu_head rcu;
+};
+
+static DEFINE_HASHTABLE(HIDDEN_NAME_HLIST, 8);
+
+static u32 susfs_name_hash(const char *name, int namlen) {
+	u32 hash = 0;
+	int i;
+	for (i = 0; i < namlen; i++)
+		hash = hash * 31 + (unsigned char)name[i];
+	return hash;
+}
+
+bool susfs_is_hidden_name(const char *name, int namlen, uid_t caller_uid) {
+	struct susfs_hidden_name_entry *entry;
+	u32 hash;
+
+	if (!name || namlen <= 0)
+		return false;
+
+	hash = susfs_name_hash(name, namlen);
+
+	rcu_read_lock();
+	hash_for_each_possible_rcu(HIDDEN_NAME_HLIST, entry, node, hash) {
+		if (entry->namlen == namlen &&
+		    !memcmp(entry->name, name, namlen) &&
+		    caller_uid != entry->owner_uid) {
+			rcu_read_unlock();
+			return true;
+		}
+	}
+	rcu_read_unlock();
+	return false;
+}
+EXPORT_SYMBOL(susfs_is_hidden_name);
